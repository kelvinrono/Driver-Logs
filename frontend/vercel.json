{
  "buildCommand": "npm run build",
  "outputDirectory": "./dist",
  "env": {
    "VITE_API_URL": "@vite_api_url",
    "VITE_API_BASE_URL": "@vite_api_base_url"
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ]
}
